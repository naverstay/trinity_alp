                setTimeout(function () {
                    let hddCount = 0;
                    let hddCount25 = 0;
                    let hddCount35 = 0;
                    let hddCountSAS = 0;
                    let hddCountSATA = 0;
                    let price = 0;
                    let descr = '';

                    if (initiator) {
                        that.updateDependencies(initiator);
                    }

                    for (let typeId in that.partTypeIds) {
                        if (that.partTypeIds.hasOwnProperty(typeId)) {
                            let item = window.preset[typeId];

                            if (!item) {
                                continue;
                            }

                            if ('list' in item) {
                                let keys = Object.keys(item.list);
                                let skipPreinstalled = false;

                                for (let ind = 0; ind < keys.length; ind++) {
                                    let component = item.list[keys[ind]];

                                    if (!component) {
                                        continue;
                                    }

                                    let info = that.getInfo(typeId, component.id);

                                    //window.dev && console.log('typeId', typeId, component, info);

                                    if (component.id.length && ind < keys.length) {
                                        let c = parseInt(component.count || 0);
                                        let itemPrice = info.price;
                                        let preinstallCorrection = 0;

                                        switch (typeId) {
                                            case 'SATA':
                                                hddCountSATA += c;
                                                break;
                                            case 'SAS':
                                                hddCountSAS += c;
                                                break;
                                            case 'SSD':
                                                if (info.formFactor === '25') {
                                                    hddCount25 += c;
                                                } else if (info.formFactor === '35') {
                                                    hddCount35 += c;
                                                } else {
                                                    hddCount += c;
                                                }
                                                break;
                                        }

                                        let count = +((component.count || 0));

                                        if (that.multiple.hasOwnProperty(typeId)) {
                                            if (!info.price && component.id == '0') {
                                                count = 0;
                                                let len = window.preset[typeId].list.length;

                                                if (len > 1 && (len - 1) > ind) {
                                                    return that.removeItem(typeId, ind);
                                                }
                                            } else if (ind === keys.length - 1 && c) {
                                                that.appendEl(typeId, c);
                                            }
                                        }

                                        descr += (count && info.description) ? ' / ' + info.description : '';

                                        if (info.price === 0) {
                                            priceRequest = true;
                                        }

                                        if (info.preinstalled && !skipPreinstalled) {
                                            skipPreinstalled = true;
                                            preinstallCorrection = 1;
                                            that.preinstalled[typeId] = item.id;
                                        }

                                        price += Math.max(0, (count - preinstallCorrection)) * itemPrice;
                                    } else {
                                        if (item.list.length > 1 && ind < item.list.length - 1) {
                                            item.list.splice(ind, 1);
                                        }
                                    }
                                }
                            } else if (item.id) {
                                let info = that.getInfo(typeId, item.id);
                                let count = +((item.count || 0));
                                let itemPrice = info.price;
                                let preinstallCorrection = 0;
                                descr += (count && info.description) ? ' / ' + info.description : '';

                                if (itemPrice === 0) {
                                    priceRequest = true;
                                }

                                if (info.preinstalled) {
                                    preinstallCorrection = 1;
                                    that.preinstalled[typeId] = item.id;
                                }

                                price += Math.max(0, (count - preinstallCorrection)) * itemPrice;
                            }
                        }
                    }

                    window.preset.totalPrice = priceRequest ? 'по запросу' : price + (window.preset.platformPrice || 0);
                    window.preset.hddCount = hddCount;
                    window.preset.hddCount25 = hddCount25;
                    window.preset.hddCount35 = hddCount35;
                    window.preset.hddCountSAS = hddCountSAS;
                    window.preset.hddCountSATA = hddCountSATA;
                    window.preset.totalDescription = descr.trim().replace(/^\//, '');
                    window.preset.totalPriceR = priceRequest ? 'по запросу' :  that.pricePrint(window.preset.exchangeRate * window.preset.totalPrice);
                    window.preset.totalPriceD = priceRequest ? 'по запросу' : that.pricePrint(window.preset.totalPrice);
                    window.preset.totalPriceItemsD = priceRequest ? 'по запросу' : that.pricePrint(window.preset.totalPrice * window.preset.itemCount);
                    window.preset.totalPriceItemsR = priceRequest ? 'по запросу' : that.pricePrint(window.preset.exchangeRate * window.preset.totalPrice * window.preset.itemCount);

                    let event = new CustomEvent("update-preset", {
                        detail: {
                            preset: window.preset
                        }
                    });

                    window.dispatchEvent(event);

                    that.checkFixedBlock()
                }, 5);

                //window.dev && console.log('updatePreset', window.preset);
